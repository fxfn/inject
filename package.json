{
  "name": "@fxfn/inject",
  "version": "0.0.1",
  "description": "Zero-dependency basic DI framework using typescript@5 decorators.",
  "type": "module",
  "packageManager": "pnpm@10.6.0",
  "main": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts"
    }
  },
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "keywords": [
    "fxfn",
    "inject",
    "@fxfn/inject",
    "di",
    "dependency-injection",
    "container"
  ],
  "scripts": {
    "test": "node --import tsx --test **/**/*.test.ts **/*.test.ts",
    "build": "tsup"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "tsup": "^8.1.0",
    "tsx": "^4.16.0",
    "typescript": "^5.0.0"
  }
}